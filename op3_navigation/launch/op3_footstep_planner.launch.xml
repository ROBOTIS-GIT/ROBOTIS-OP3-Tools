<?xml version="1.0"?>
<launch>
  <include file="$(find-pkg-share op3_navigation)/launch/map_server_empty.launch.xml" />
  <node pkg="footstep_planner" exec="footstep_planner_node" name="footstep_planner" output="screen">
    <param from="$(find-pkg-share op3_navigation)/config/planning_params.yaml" />
    <param from="$(find-pkg-share op3_navigation)/config/planning_params_robotis_op3.yaml" />
    <param from="$(find-pkg-share op3_navigation)/config/footsteps_robotis_op3.yaml" />
    <remap from="map" to="projected_map" />
  </node>
</launch>
